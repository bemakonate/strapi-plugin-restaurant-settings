{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "product"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true,
    "increments": true,
    "comment": ""
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "price": {
      "type": "decimal",
      "default": "0.00"
    },
    "description": {
      "type": "text"
    },
    "categories": {
      "plugin": "restaurant-settings",
      "via": "products",
      "collection": "category"
    },
    "sideProducts": {
      "plugin": "restaurant-settings",
      "collection": "side-product",
      "via": "products",
      "dominant": true
    }
  }
}